    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Roboto+Condensed:400" />

    @Styles.Render("~/styles/bootstrap")
    @Styles.Render("~/styles/updated")
